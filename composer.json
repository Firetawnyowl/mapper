{
    "name": "tarantool/mapper",
    "description": "PHP Object Mapper for Tarantool.",
    "keywords": ["tarantool", "client", "pure", "nosql", "mapper"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "tarantool/client": ">=0.8.0"
    },
    "suggest": {
        "ext-msgpack": "For using PeclPacker.",
        "rybakit/msgpack": "For using PurePacker.",
        "phpdocumentor/reflection-docblock": "Annotation Plugin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2.5",
        "rybakit/msgpack": "^0.7.0",
        "phpdocumentor/reflection-docblock": "^5.1.0",
        "nesbot/carbon": "^2.35.0",
        "psr/log": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        },
        "classmap": ["tests"]
    },
    "autoload": {
        "psr-4": {
            "Tarantool\\Mapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dmitry Krokhin",
            "email": "nekufa@gmail.com"
        }
    ]
}
